@section owlCss{
    <link rel="stylesheet" href="~/css/owl-carousel/owl.carousel.css">
    <link rel="stylesheet" href="~/css/owl-carousel/owl.theme.default.css">
}

@section preloaderScript{
    <script type="text/javascript">
        $(window).on('load', function () {
            $('.spinner').delay(1500).fadeOut("slow", function () {
                $(this).css('z-index', '0');
                $('.preloader').addClass('complete').css("z-index", "0");
            });
        });
    </script>
}

@section preloader{
    <!-- preloader -->
    <div class="preloader">
        <div class="spinner">
            <div class="shadow"></div>
            <div class="box"></div>
        </div>
    </div>
    <!-- end of preloader -->
}

@section owlScript{
    <script src="~/script/owl-carousel/owl.carousel.js"></script>
}

@section Scripts{
    <script>
        function initMap() {
            var test = { lat: 40.374090, lng: 49.852436 };
            var map = new google.maps.Map(document.getElementById('map'), {
                zoom: 15,
                center: test
            });
            var marker = new google.maps.Marker({
                position: test,
                map: map
            });
        }
    </script>
    <script async defer
            src="https://maps.googleapis.com/maps/api/js?key=AIzaSyC8EdJxYPClDOkk4tzALsjm-OkjKA59fjQ&language=az&region=AZ&callback=initMap">
    </script>
}